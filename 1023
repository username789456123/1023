#include <stdio.h>
int main()
{
	int num[10];
	int i,j,index;
	for(i=0;i<10;i++)
	{
		if(num[i]!=0){
			printf("%d",i);
			index = i;
			break;
		}
	}
	for(i=0;i<10;i++){
		if(i==index)
		{
			for(j=0;j<num[i]-1;j++)
			printf("%d",i);
		}
		else
		{
			if(num[i]!=0){
				for(j=0;j<num[i]:j++)
				printf("%d",i);
			}
		}
	}
	return 0;
}
